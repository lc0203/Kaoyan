<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibenate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="people.All_information" table="information">
		<id name="id" column="user_id">
			<generator class="assigned"></generator>
		</id>
		<property name="name" type="string"></property>
		<property name="age" type="string"></property>
		<property name="email" type="string"></property>
		<property name="area" type="string"></property>
		<property name="school1" type="string"></property>
		<property name="school2" type="string"></property>
		<property name="major" type="string"></property>
		<property name="sex" type="string"></property>
		<property name="year" type="string"></property>
		<property name="produce" type="string"></property>
	</class>

</hibernate-mapping>